{
  "name": "api-v1",
  "description": "api without framework",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "filp/whoops": "^2.1",
    "league/fractal": "^0.12.0",
    "illuminate/routing": "5.6.23"
  },
  "require-dev": {
  },
  "autoload": {
    "files": [
      "core/Helpers/PathsHelper.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Core\\": "core/"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
